{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["plugin:@typescript-eslint/recommended", "airbnb-base", "prettier"],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-console": "warn"
  }
}
