{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["plugin:@typescript-eslint/recommended", "airbnb-base", "prettier"],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier", "eslint-plugin-import", "eslint-plugin-prettier"],
  "settings": {
    "import/resolver": {
      "typescript": {}
    }
  },
  "globals": {
    "describe": "readonly",
    "it": "readonly",
    "before": "readonly",
    "after": "readonly"
  },
  "rules": {
    "no-console": "warn"
  }
}
