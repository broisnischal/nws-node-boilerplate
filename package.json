  "main": "index.js",
  "scripts": {
    "ts-dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "start": "node dist/index.js",
    "build": "npm run prettier:check && npm run lint && tsc --project ./tsconfig.build.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "prod": "node dist/index.js",
    "bprod": "npm run build && node dist/index.js",
    "lint": "eslint \"./src/**/*.ts\"",
    "lint-fix": "eslint src/ --ext .ts --fix",
    "prettier": "prettier --config .prettierrc \"./src/**/*.+(js|json|ts)\" --write --ignore-unknown",
    "prettier:check": "prettier --config .prettierrc \"./src/**/*.+(js|json|ts)\" --check --ignore-unknown",
    "test": "mocha -r tsconfig-paths/register -r ts-node/register  \"./src/test/**/*.ts\" --exit",
    "post-commit-action": "git add .",
    "docs": "npx ts-node-dev docs/index.ts"
  },
  "lint-staged": {
    "./src/**/*.+(ts|js)": [
      "npm run lint",
      "npm run prettier:check",
      "npm run test"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "npm run post-commit-action"
    }
  },
